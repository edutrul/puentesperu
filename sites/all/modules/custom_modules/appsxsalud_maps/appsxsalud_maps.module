<?php
/**
 * @file contents for appsxsalud_maps.module
 */

/**
 * Implements hook_views_pre_render().
 */
function appsxsalud_maps_views_pre_render(&$view) {
  if ($view->name == 'mapa_campanias' && $view->current_display == 'page') {
    drupal_add_js(drupal_get_path('module', 'appsxsalud_maps') . '/js/appsxsalud-maps.js');
  }
}